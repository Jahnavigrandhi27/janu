import java.util.*;
import java.io.*;
class Main
    {
        public static void main(String args[])
        {
        Scanner sc=new Scanner(System.in);
        int n;
        System.out.print("Enter number:");
        n=sc.nextInt();
        fact(n);
        long s=fact(n);
        System.out.println("Factorial of "+n+" is:"+s);
        }
      public static long fact(int x)
        {
        if(x<=1)
          return 1;
        else
          return x*fact(x-1);
        }
    }
