import java.util.*;
import java.io.*;
 class Main
{
public static void main(String[] args)
{
        int sum=0;
        Scanner s=new Scanner(System.in);
        System.out.print("Enter string:");
        String str=s.nextLine();
        int len=str.length();
          for(int i=0;i<len;i++)
            {
              if(str.charAt(i)==' ')
              {
               continue;
              }
               sum++;
            }
         System.out.println(sum);
           
    }
   
}
