import java.util.*;
 class Main
{
public static void main(String[] args)
{
int i;
String str,str1;
String rev="";
Scanner sc=new Scanner(System.in);
System.out.print("Enter String:");
str=sc.next();
str1=str;
int len=str.length();
for(i=len-1;i>=0;i--)
{
   rev=rev+str.charAt(i);
}
if(str1.equals(rev))
   System.out.print(str1+" is Pallendrome");
else
System.out.print(str1+" is Not Pallendrome");
}
}

