

import java.util.Scanner;
public class Main {

 public static void main(String[] args) {
    Scanner cs=new Scanner(System.in);
    int num;
    System.out.println("Enter the number:");
    num=cs.nextInt();
    int odd=0,even=0;
    while(num!=0)
    {
        int rem=num%10;
        if(rem%2==0)
            even=even*10+rem;
        else
            odd=odd*10+rem;
        num=num/10;
    }
   System.out.println(+even);
   System.out.println(+odd);
 }
}
